<%- include header %>
<% articles.forEach(function (article, index) { %>
<p><h2><a href="u/<%= article.name %>/<%= article.time.day %>/<%= article.title %>"><%= article.title %></a></h2></p>
<p class="info">
    作者：<a href="u/<%= article.name %>"><%= article.name %></a> |
    日期：<%= article.time.minute %>
</p>
<p><%- article.content %></p>
<% }) %>
<%- include page %>
<%- include footer %>